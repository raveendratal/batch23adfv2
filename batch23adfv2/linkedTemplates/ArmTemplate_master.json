			"name": "batch23adfv2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azure_blob_target_connectionString": {
						"value": "[parameters('ls_azure_blob_target_connectionString')]"
					},
					"ls_oracle_onprem_source_connectionString": {
						"value": "[parameters('ls_oracle_onprem_source_connectionString')]"
					},
					"LinkedSelfHosted-IR_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('LinkedSelfHosted-IR_properties_typeProperties_linkedInfo_resourceId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}