{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "batch23adfv2"
		},
		"ls_azure_blob_target_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azure_blob_target'"
		},
		"ls_azure_synapse_sqldwh_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azure_synapse_sqldwh'"
		},
		"ls_onprem_file_source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_onprem_file_source'"
		},
		"ls_oracle_onprem_source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_oracle_onprem_source'"
		},
		"ls_adlsgen2_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsgen2batch23.dfs.core.windows.net"
		},
		"ls_azure_kv_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://batch23akv.vault.azure.net/"
		},
		"ls_onprem_file_source_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "E:\\files\\SH"
		},
		"ls_onprem_file_source_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "raveendra"
		},
		"SelfHostedShared-IR_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/e0bf1971-b822-46ca-8e4a-72555f0a959e/resourcegroups/rg-batch24-de-dev/providers/Microsoft.DataFactory/factories/batch24adfv2/integrationruntimes/Azure-SelfHosted-IR"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "batch23adfv2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azure_blob_target_connectionString": {
						"value": "[parameters('ls_azure_blob_target_connectionString')]"
					},
					"ls_azure_synapse_sqldwh_connectionString": {
						"value": "[parameters('ls_azure_synapse_sqldwh_connectionString')]"
					},
					"ls_onprem_file_source_password": {
						"value": "[parameters('ls_onprem_file_source_password')]"
					},
					"ls_oracle_onprem_source_connectionString": {
						"value": "[parameters('ls_oracle_onprem_source_connectionString')]"
					},
					"ls_azure_kv_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_azure_kv_properties_typeProperties_baseUrl')]"
					},
					"SelfHostedShared-IR_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('SelfHostedShared-IR_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"ls_adlsgen2_source_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsgen2_source_properties_typeProperties_url')]"
					},
					"ls_onprem_file_source_properties_typeProperties_host": {
						"value": "[parameters('ls_onprem_file_source_properties_typeProperties_host')]"
					},
					"ls_onprem_file_source_properties_typeProperties_userId": {
						"value": "[parameters('ls_onprem_file_source_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "batch23adfv2_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'batch23adfv2_ArmTemplate_0')]"
			]
		}
	]
}